User Function FIPCOL10()

_nValor:=0

IF((SUBSTR(CT2->CT2_CREDIT,1,6) == "120299" .OR. SUBSTR(CT2->CT2_CREDIT,1,6) == "120301" .OR. SUBSTR(CT2->CT2_CREDIT,1,6) == "120303" .OR. SUBSTR(CT2->CT2_CREDIT,1,6) == "120399" .OR. SUBSTR(CT2->CT2_CREDIT,1,6) == "120401" .OR. SUBSTR(CT2->CT2_CREDIT,1,6) == "120499") .AND. CT2->CT2_TPSALD=="1")
	_nValor:=CT2->CT2_VALOR
eLSE
	_nValor:=0
Endif

Return(_nValor)

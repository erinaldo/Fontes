User Function FICOMR01()
Local lRet := .T.
Local aPerguntas := {}
Local aRetorno := {}

aAdd(aPerguntas, {1, "Cotação de....", Space(TamSX3("C8_NUM")[1]), "", , "FILSC8", , TamSX3("C8_NUM")[1], .T., , .T.})
aAdd(aPerguntas, {1, "Cotação até...", Space(TamSX3("C8_NUM")[1]), "", , "FILSC8", , TamSX3("C8_NUM")[1], .T., , .T.})

If !(ParamBox(aPerguntas, "Impressão de Mapa de Cotação", @aRetorno))
	lRet := .F.
Endif

If (lRet)
	Processa({|| ImpRel()}, "Processando Arquivos...")
Endif
Return(lRet)


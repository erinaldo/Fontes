		_xQuery := ChangeQuery(_xQuery)
		dbUseArea(.T.,"TOPCONN",TCGENQRY(,,_xQuery),"TRBVAL",.t.,.t.)

		_lNegrito := .F.
		
		If TRBVAL->FORN == SC8->C8_FORNECE

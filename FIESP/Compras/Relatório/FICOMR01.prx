			Else
				oPrint:Say(nLinha, 2218, AllTrim(Transform(SC8->C8_TOTAL, PesqPict("SC8", "C8_TOTAL"))), oFont7)
			EndIf
			
		Elseif (nForn == 4)
//			If !((SC8->C8_NUMPED == "XXXXXX") .Or. (Empty(SC8->C8_NUMPED)))

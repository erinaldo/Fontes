			oPrint:Say(0205, 1048, Posicione("SA2", 1, xFilial("SA2") + cFornece + cLoja, "A2_NREDUZ"), oFont4)

			oPrint:Say(2079, 1048, AllTrim(cCondPag), oFont4)
			oPrint:Say(2129, 1048, AllTrim(Transform(cPrazo, PesqPict("SC8", "C8_PRAZO"))), oFont4)


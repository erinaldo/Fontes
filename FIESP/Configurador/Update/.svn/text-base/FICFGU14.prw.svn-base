	'ZH_FILIAL+ZH_NUMERO+ZH_TIPO+ZH_BANCO+ZH_AGENCI+ZH_CONTA'				, ; //CHAVE
	'Numero+Operacao+Banco+Agencia+Conta Banco'								, ; //DESCRICAO
	'Numero+Operacion+Banco+Agencia+Cuenta Banco'							, ; //DESCSPA

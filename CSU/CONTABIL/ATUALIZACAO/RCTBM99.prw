dbSelectArea("SD1")
dbSetOrder(1)//D1_FILIAL+D1_DOC+D1_SERIE+D1_FORNECE+D1_LOJA+D1_COD+D1_ITEM
If dbSeek(cChave,.F.)
	cRetorno	:= STRTRAN(SD1->D1_XDTAQUI,"/","")
	cRetorno	:= Substr(cRetorno,3,4)+Substr(cRetorno,1,2)
Endif


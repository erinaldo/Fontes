Function U_RZB7ULTR(cCodRat,cAnoMes,lAtiva)



Local aArea		:= GetArea()
Local cRetorno	:= ""

Local cQry		:= ""




cQry	:= "SELECT TOP 1 (ZB7_REVISA) TOP_REVISAO FROM "+RetSqlName("ZB7")+" "+Chr(13)+Chr(10)

cQry	+= "WHERE ZB7_FILIAL = '"+xFilial("ZB7")+"' "+Chr(13)+Chr(10)
cQry	+= "AND ZB7_CODRAT = '"+cCodRat+"' "+Chr(13)+Chr(10)
cQry	+= "AND ZB7_ANOMES = '"+cAnoMes+"' "+Chr(13)+Chr(10)
If lAtiva
	cQry	+= "AND ZB7_ATIVO = 'A' "+Chr(13)+Chr(10)
Endif
cQry	+= "AND D_E_L_E_T_ <> '*' "+Chr(13)+Chr(10)

cQry	+= "ORDER  BY ZB7_FILIAL, ZB7_CODRAT, ZB7_ANOMES, ZB7_REVISA DESC "+Chr(13)+Chr(10)


If Select("TMPTOP") > 0
	DbSelectArea("TMPTOP")
	DbCloseArea()
Endif

MsAguarde({|| DbUseArea( .T. , "TOPCONN", TCGenQry(,,cQry),"TMPTOP", .F. , .T. )}, "Verificando a última revisão da tabela de rateio...")




cRetorno	:=  TMPTOP->TOP_REVISAO

RestArea(aArea)
Return cRetorno















Function U_RZB7REVA(cCodRat,cAnoMes)



Local aArea		:= GetArea()
Local lRetorno	:= .F. 
Local cQry		:= ""




If IsInCallStack("MATA103")

	cAnoMes		:= U_GetCompetencia(SF1->F1_FILIAL+SF1->F1_DOC+SF1->F1_SERIE+SF1->F1_FORNECE+SF1->F1_LOJA)
Endif




cQry	:= "SELECT COUNT(*) CONTAGEM FROM "+RetSqlName("ZB7")+" "+Chr(13)+Chr(10)
cQry	+= "WHERE ZB7_FILIAL = '"+xFilial("ZB7")+"' "+Chr(13)+Chr(10)
cQry	+= "AND ZB7_CODRAT = '"+cCodRat+"' "+Chr(13)+Chr(10)
cQry	+= "AND ZB7_ANOMES = '"+cAnoMes+"' "+Chr(13)+Chr(10)
cQry	+= "AND ZB7_ATIVO = 'A' "+Chr(13)+Chr(10)
cQry	+= "AND D_E_L_E_T_ <> '*' "

If Select("TMPCON") > 0
	DbSelectArea("TMPCON")
	DbCloseArea()
Endif

MsAguarde({|| DbUseArea( .T. , "TOPCONN", TCGenQry(,,cQry),"TMPCON", .F. , .T. )}, "Verificando a última revisão da tabela de rateio...")

If TMPCON->CONTAGEM > 0
	lRetorno	:= .T. 
Else
	Aviso("Aviso","Não existe revisão ativa para a tabela de rateio selecionada nesse período. Por favor, verifique o cadastro de tabelas de rateio.",{"OK"},,"Atenção",,"BMPPERG")
Endif

RestArea(aArea)
Return lRetorno

















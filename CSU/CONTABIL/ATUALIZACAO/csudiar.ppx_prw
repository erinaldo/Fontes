			nTotDiaC	:= 0
			li+=2
		EndIF
	EndDO
	If lEnd
		Exit
	End

	IF (nTotMesd+nTotMesc) > 0
		PrintOut(li,055,"Total do Mês ",)
		ValorCTB(nTotMesD,li,nColDeb,nTamDeb,nDecimais, .F. ,cPicture,"1")
		ValorCTB(nTotMesC,li,nColCrd,nTamCrd,nDecimais, .F. ,cPicture,"2")

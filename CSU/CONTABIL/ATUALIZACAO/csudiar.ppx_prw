	PrintOut(li,055,"Total Geral ",)
	ValorCTB(nTotDeb ,li,nColDeb,nTamDeb,nDecimais, .F. ,cPicture,"1")
	ValorCTB(nTotCred,li,nColCrd,nTamCrd,nDecimais, .F. ,cPicture,"2")

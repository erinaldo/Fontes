				EndIf
				If TRD2->CT2_DC == "2" .Or.  TRD2->CT2_DC == "3"
					nTotCred += TRD2->CT2_VALOR
					nTotdiaC += TRD2->CT2_VALOR
					nTotMesC += TRD2->CT2_VALOR

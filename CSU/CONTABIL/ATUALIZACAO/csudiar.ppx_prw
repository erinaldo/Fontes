	dbClearFilter()
	Return
Endif





If !ct040Valid(mv_par04)
	lRet := .F. 
Else
	aSetOfBook := CTBSetOf(mv_par04)
EndIf

If lRet
	aCtbMoeda	:= CtbMoeda(mv_par03)

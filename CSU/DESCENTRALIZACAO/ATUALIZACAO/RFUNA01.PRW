User Function xValPcE2()

Local lRet := .T.
Local aArea := GetArea()

dbSelectArea("SC7")
dbSetOrder(3)
If !dbSeek(xFilial("SC7")+M->E2_FORNECE+M->E2_LOJA+M->E2_NUMPC)
	lRet := .F.
	Help(' ', 1, 'NAOPODE',,"Este pedido de compra nใo pertence ao fornecedor informado no tํtulo.",1,0)
Else
	If lRet
		If SC7->C7_CONAPRO = 'L'

User Function RetCtaPA(cSE2Num,cSE2Pref,cSE2Parc,cSE2Tip,cSE2For,cSE2Loj)

Local aArea    := GetArea()
Local aAreaSE2 := SE2->( GetArea() )
Local aAreaSE5 := SE5->( GetArea() )
Local aAreaSA6 := SA6->( GetArea() )
Local cConta   := ""
Local c_Query  := ""

c_Query := " SELECT E5_BANCO, E5_AGENCIA, E5_CONTA "
c_Query += " FROM "+RetSqlName("SE5")               "
c_Query += " WHERE E5_FILIAL = '"+xFilial("SE5")+"' "
c_Query += " AND E5_PREFIXO  = '"+cSE2Pref+"'          "
c_Query += " AND E5_NUMERO  = '"+cSE2Num+"'          "
c_Query += " AND E5_PARCELA  = '"+cSE2Parc+"'          "
c_Query += " AND E5_TIPO  = '"+cSE2Tip+"'          "
c_Query += " AND E5_CLIFOR  = '"+cSE2For+"'          "
c_Query += " AND E5_LOJA  = '"+cSE2Loj+"'          "
c_Query += " AND D_E_L_E_T_  = ''          "

U_MontaView( c_Query, "TMP" )

dbSelectArea("TMP")
TMP->( DbGoTop() )

dbSelectArea("SA6")
dbSetOrder(1)

If dbSeek( xFilial("SA6")+TMP->E5_BANCO+TMP->E5_AGENCIA+TMP->E5_CONTA)
	cConta := SA6->A6_CONTA
EndIf

TMP->( DbCloseArea() )
RestArea(aAreaSE2)
RestArea(aAreaSE5)
RestArea(aAreaSA6)
RestArea(aArea)

Return(cConta)



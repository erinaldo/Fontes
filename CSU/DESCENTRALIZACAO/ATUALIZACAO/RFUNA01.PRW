Local _aArea := GETAREA()
Local _nDias := GetMv("MV_DIAVENC")
Local _nPosVENCTO  	:= 1, _nPValor := 0
Local _lRet			:= .T.
Local _cTITULO 		:= "AtenวรO"

/*ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
ณ                 Caso seja inclusao de Titulos a Pagar                      ณ
ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู*/

If Funname() $ "FINA050;FINA750" .Or. _aColsE2 == Nil
	/*ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	ณ              Verifica se o Vencimento eh Inferior ao Permitido             ณ
	ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู*/
	If Funname() $ "MATA103"
		
		If cCondicao <> '001'
			If dDataBase + _nDias > M->E2_VENCTO
				// Alteradas as duas linhas abaixo por Flavio Novaes em 02/04/07 - Chamado nr. 000000001227
				//_lRet := .F.
				//Aviso( "Aten็ใo ! Parametro MV_DIAVENC" , "O vencimento nใo atende o fluxo interno da CSU. Favor Solicitar imediatamente carta de prorroga็ใo, incluir o documento com o vencimento prorrogado e enviar o documento e carta de prorroga็ใo para o Depto. Fiscal - Alphaville (Att. Jane)." , {"Ok"} , 2 , "Solicite Prorroga็ใo !" )
				IF SELECT('SZU7') > 0
					dbSelectArea('SZU7')
					dbCloseArea()
				ENDIF
				
				_cQuery := " SELECT * FROM "+RETSQLNAME('ZU7')
				_cQuery += " WHERE D_E_L_E_T_ <> '*' "
				_cQuery += " AND   ZU7_FORNEC = '"+M->CA100FOR+"' "
				_cQuery += " AND   ZU7_LOJA = '"+M->CLOJA+"' "
				_cQuery += " AND ZU7_ATIVO = 'T' AND ZU7_TIPOLI = '2' "
				
				TCQUERY _cQuery NEW ALIAS 'SZU7'
				
				IF EOF()
					_lRet := .F.
					Aviso( "Aten็ใo ! Parametro MV_DIAVENC" , "O vencimento nใo atende o fluxo interno da CSU. Favor Solicitar imediatamente carta de prorroga็ใo, incluir o documento com o vencimento prorrogado e enviar o documento e carta de prorroga็ใo para o Depto. Fiscal - Alphaville (Att. Jane)." , {"Ok"} , 2 , "Solicite Prorroga็ใo !" )
				ENDIF
				dbSelectArea('SZU7')
				dbCloseArea()
				// Alterado por Flavio Novaes em 02/04/07 ate aqui.
			Endif
			
		Endif
		
		/*/ Comentado em 29/01/07 por Leonardo Soncin - Chamado nr. 000000000755
	Else
		
		If dDataBase + _nDias > M->E2_VENCTO
			_lRet := .F.
			Aviso( "Aten็ใo ! Parametro MV_DIAVENC" , "O vencimento nใo atende o fluxo interno da CSU. Favor Solicitar imediatamente carta de prorroga็ใo, incluir o documento com o vencimento prorrogado e enviar o documento e carta de prorroga็ใo para o Depto. Fiscal - Alphaville (Att. Jane)." , {"Ok"} , 2 , "Solicite Prorroga็ใo !" )
		Endif
		*/
		
		// Acrescentado por Flavio Novaes em 30/03/07 - Chamado nr. 000000001128
	Else
		IF lAltera
			_lRet := !(ALLTRIM(M->E2_NATUREZ)$ALLTRIM(GETMV("MV_IRF")) .OR.;
			ALLTRIM(M->E2_NATUREZ)$ALLTRIM(GETMV("MV_ISS")) .OR.;
			ALLTRIM(M->E2_NATUREZ)$ALLTRIM(GETMV("MV_INSS")))
			AVISO("Aten็ใo! Nใo Alterar Vencimento!","Nใo ้ permitido alterar o Vencimento de Tํtulos dos Impostos: "+CHR(13)+CHR(10)+"ISS, INSS e IRRF.",{"Ok"},2,"")
		ENDIF
		
		// Acrescentado por Douglas David em 09/08/13 - OS 1334/13
		
		If lAltera .And. Upper( AllTrim(SE2->E2_XLIBNIV) )$ '2/3'
			_lRet := !Upper( AllTrim(SE2->E2_XLIBNIV) ) $ '2/3'
			AVISO("Aten็ใo! Nใo Alterar Vencimento!","Nใo ้ permitido alterar o Vencimento de Tํtulos CONFERIDOS! "+CHR(13)+CHR(10)+"Para Altera็ใo de Vencimento ้ obrigat๓rio retirar a CONFERENCIA!",{"Ok"},2,"")
		EndIf
		
		// Acrescentado por Alexandre Eduardo em 25/09/13 - OS 2810/13
		
		If lAltera .And. (dDataBase <> SE2->E2_EMIS1)
			_lRet := .F.
			AVISO("Aten็ใo! Nใo Alterar Vencimento!","Nใo ้ permitido alterar o Vencimento de Tํtulos com a data base do sistema diferente da data de contabiliza็ใo"+CHR(13)+CHR(10),{"OK"},2,"")
		EndIf
	Endif
	
Else
	
	/*ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	ณ                 Caso seja inclusao de Documento de Entrada                 ณ
	ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู*/
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Verifica a posicao dos campos E2_VENCTO e E2_VENCREA dentro do aHeader ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	
	_nPosVENCTO := aScan(_aHeadE2,{|x| Upper(Alltrim(x[2])) == "E2_VENCTO"})
	_nPValor    := aScan(_aHeadE2,{|x| Upper(AllTrim(x[2])) == "E2_VALOR"})
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Verifica cada linha do aCols               ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	For _i := 1 to Len(_aColsE2)
		If _aColsE2[_i,Len(_aHeadE2)+1] == .F.  // Linha ativa
			
			//		ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
			//		ณ Valida Vencimento                          ณ
			//		ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
			If cCondicao <> '001'
				
				if (dDataBase + _nDias) > _aColsE2[_i,_nPosVENCTO] .And. _aColsE2[_i][_nPValor] > 0
					// Alteradas as duas linhas abaixo por Flavio Novaes em 02/04/07 - Chamado nr. 000000001227
					// _lRet := .F.
					// Aviso( "Aten็ใo ! Parametro MV_DIAVENC" , "A Data de Vencimento do Titulo a Pagar ้ Invแlida ! Nใo serแ possํvel incluir o Titulo a Pagar com este Vencimento. Solicite prorroga็ใo ao Fornecedor !" , {"Ok"} , 1 , "Solicite Prorroga็ใo !" )
					IF SELECT('SZU7') > 0
						dbSelectArea('SZU7')
						dbCloseArea()
					ENDIF
					// Sergio em abr/2009: Correcao da query que estava com aspas duplas:
					_cQuery := " SELECT * FROM "+RETSQLNAME('ZU7')
					_cQuery += " WHERE D_E_L_E_T_ = ' ' "
					_cQuery += " AND   ZU7_FORNEC = '"+M->CA100FOR+"' "
					_cQuery += " AND   ZU7_LOJA   = '"+M->CLOJA+"' "
					_cQuery += " AND   ZU7_ATIVO  = 'T' AND ZU7_TIPOLI = '2'"
					TCQUERY _cQuery NEW ALIAS 'SZU7'
					IF EOF()
						_lRet := .F.
						Aviso("Aten็ใo! Parametro MV_DIAVENC","A Data de Vencimento do Titulo a Pagar ้ Invแlida! Nใo serแ possํvel incluir o Titulo a Pagar com este Vencimento! Solicite prorroga็ใo ao Fornecedor!",{"Ok"},1,"Solicite Prorroga็ใo!")
					ENDIF
					dbSelectArea('SZU7')
					dbCloseArea()
					// Alterado por Flavio Novaes em 02/04/07 ate aqui.
				EndIf
			Endif
		Endif
	Next
Endif
RESTAREA(_aArea)
Return _lRet


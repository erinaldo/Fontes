User Function VldNatFor( cNaturez , cFornecedor , cLoja )

/*ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
ณ              Declara as Variaveis Locais Utilizadas na Rotina              ณ
ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู*/

Local lRet       := .T.
Local cEol       := Chr(13)+Chr(10)
Local cNextAlias := GetNextAlias()
Local _aArea     := GetArea()
Local cQuery     := ""
Local cNatSZH    := " "
Local cNatSED    := " "
PRIVATE CA100FOR
/*ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
ณ   Caso a Natureza ou o Fornecedor esteja em Branco nao Executa a Funcao    ณ
ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู*/

If IsInCallStack("A103Devol") .Or. IsInCallStack("MATA140") // O.S 3041/15
	Return(lRet)
EndIf


If Empty(cNaturez) .Or. Empty(cFornecedor) .Or. Empty(cLoja)
	Return .t.
Endif

/*ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
ณ            Abre a Tabela de Amarracao entre Natureza x Fornecedores e a Tabela de Naturezas       ณ
ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู*/

DbSelectArea("SZH")
DbSetOrder(1)       // Natureza + Fornecedor + Loja
DBGotop()

// === Tatiana Barbosa - 26/05/20 - OS 1235/10

DbSelectArea("SED")
DbSetOrder(1)      		// Filial + C๓digo
DbGotop()
// === Tatiana Barbosa - 26/05/20 - OS 1235/10


/*ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
ณ       Posiciona no Cadastro de Amarracao entre Natureza x Fornecedores     ณ
ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู*/
cQuery := " SELECT R_E_C_N_O_  AS REGS "
cQuery += " FROM SZH050 WHERE ZH_FILIAL = '  ' "
cQuery += " AND ZH_NATUREZ = '"+cNaturez+"' "
cQuery += " AND ZH_FORCOD = '"+cFornecedor+"' "
cQuery += " AND ZH_FORLJ = '"+cLoja+"' "
cQuery += " AND D_E_L_E_T_ = ' ' "

U_MontaView(cQuery , cNextAlias)

(cNextAlias)->(DbGotop())

//If !DbSeek( xFilial("SZH") + cNaturez + cFornecedor + cLoja )
If (cNextAlias)->REGS == 0
	//cCpos := " || StrZero(SZH->(IndexOrd()),1) => "+StrZero(SZH->(IndexOrd()),1)+" - AllTrim(SZH->( IndexKey() )) => "+AllTrim(SZH->( IndexKey() ))+" - xFilial('SZH') => "+xFilial('SZH')+" -  cNaturez = > ["+cNaturez+"] - cFornecedor => ["+cFornecedor+"] - cLoja => ["+cLoja+"]"
	Aviso( "Aten็ใo !" , "Voc๊ estแ utilizando uma natureza financeira que nใo estแ amarrada ao fornecedor ! Realize a amarra็ใo fornecedor x natureza ou selecione outra natureza financeira." , {"Ok"} , 3 , "Amarra็ใo Fornecedor x Natureza",,"PcoLock" )
	lRet := .F.
Else
	SZH->(DbGoTo((cNextAlias)->REGS))
	/*ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	ณ              Verifica se a Amarracao Esta ou Nao Bloqueada                 ณ
	ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู*/
	
	If SZH->ZH_MSBLQL == "1" // Amarra็ใo Bloqueada
		Aviso( "Aten็ใo !" , "A Amarra็ใo Fornecedor x Natureza estแ Bloqueada. Solicite o Desbloqueio !" , {"Ok"} , 1 , "Amarra็ใo Bloqueada" )
		lRet := .F.
		
		// === Tatiana Barbosa - 26/05/20 - OS 1235/10
	Else
		cNatSZH := SZH->ZH_NATUREZ
		SED->(DbSeek( xFilial("SED") + cNatSZH ))
		
		If  SED->ED_MSBLQL == "1" .Or. !AllTrim(SED->ED_TPNAT) == "A"        //Naturezas Desbloqueadas e Analํticas
			Aviso( "Aten็ใo !" , "Verifique o cadastro da Natureza " + Alltrim(SED->ED_CODIGO) + " - " + Alltrim(SED->ED_DESCRIC) + "." + ENTER + "S๓ poderใo ser utilizadas naturezas Analํticas e Desbloqueadas", {"Ok"} , 1 , "Natureza Invแlida" )
			lRet := .F.
		EndIf
		// === Tatiana Barbosa - 26/05/20 - OS 1235/10
		
	Endif
	
Endif

(cNextAlias)->(DbCloseArea()) // OS 1694/15 By Douglas David

RestArea(_aArea)

Return lRet


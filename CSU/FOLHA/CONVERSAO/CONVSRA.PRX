           RECLOCK("SRA",.F.) 
            If SRA->RA_GS $ "25*26"  
               SRA->RA_ASMEDIC := "07"
            ElseIf SRA->RA_GS $ "27*28*29*30"   
               SRA->RA_ASMEDIC := "08"
            Endif
           MSUNLOCK()               
           DBSELECTAREA("ZZ5")   		
        Endif
             
        IF Dbseek(STRZERO(ZZ1->CODCOLIG,2) +ZZ1->CHAPA+ "0396") // ASS. MED. UNIMED MASTER
           DBSELECTAREA("SRA")       
           RECLOCK("SRA",.F.) 
            If !SRA->RA_CATFUNC $ "E*G"
               SRA->RA_ASMEDIC := "03" 
            Else    
               SRA->RA_ASMEDIC := "06" 
            Endif    
           MSUNLOCK()               
           DBSELECTAREA("ZZ5")   		
        Endif
             
        IF Dbseek(STRZERO(ZZ1->CODCOLIG,2) + ZZ1->CHAPA+ "0397") // ASS. MED. UNIMED PLENO
           DBSELECTAREA("SRA")       
           RECLOCK("SRA",.F.) 
           If !SRA->RA_CATFUNC $ "E*G"
               SRA->RA_ASMEDIC := "02"
           Else 
               SRA->RA_ASMEDIC := "05"
           Endif     
           MSUNLOCK()               
           DBSELECTAREA("ZZ5")   		
        Endif
 
        IF Dbseek(STRZERO(ZZ1->CODCOLIG,2) + ZZ1->CHAPA+ "0302")  // ASS. MED. UNIMED BH
           DBSELECTAREA("SRA")       
           RECLOCK("SRA",.F.) 

               SRA->RA_ASMEDIC := "17" 	
               SRA->RA_CDODONT := "03" // ODONTO BH 
           MSUNLOCK()               
           DBSELECTAREA("ZZ5")   		
        Endif
             
        IF Dbseek(STRZERO(ZZ1->CODCOLIG,2) + ZZ1->CHAPA+ "0407")  // ASS. MED. UNIPART 
           DBSELECTAREA("SRA")       
           RECLOCK("SRA",.F.) 

               SRA->RA_ASMEDIC := "09"

           MSUNLOCK()               
           DBSELECTAREA("ZZ5")   		
        Endif

        IF Dbseek(STRZERO(ZZ1->CODCOLIG,2) + ZZ1->CHAPA+ "0304") // ODONTO
           DBSELECTAREA("SRA")       
           RECLOCK("SRA",.F.) 

               SRA->RA_CDODONT := "01"

           MSUNLOCK()               
           DBSELECTAREA("ZZ5")   		
        Endif
                               
        IF Dbseek(STRZERO(ZZ1->CODCOLIG,2) + ZZ1->CHAPA+ "0331") // ODONTOPREV
           DBSELECTAREA("SRA")       
           RECLOCK("SRA",.F.) 

               SRA->RA_CDODONT := "02"

           MSUNLOCK()               
           DBSELECTAREA("ZZ5")   		
        Endif
        
        IF Dbseek(STRZERO(ZZ1->CODCOLIG,2) + ZZ1->CHAPA+ "0305") // P. SEGURO 
           DBSELECTAREA("SRA")       
           RECLOCK("SRA",.F.) 
            
            If  SRA->RA_CATFUNC = "P"             
                SRA->RA_SEGUROV := "01"
            Else    
				SRA->RA_SEGUROV := "02"
			Endif 	                   
			
           MSUNLOCK()               
           DBSELECTAREA("ZZ5")   		
        Endif
             

        IF Dbseek(STRZERO(ZZ1->CODCOLIG,2) + ZZ1->CHAPA+ "0341") // P. SEGURO - BH 
           DBSELECTAREA("SRA")       
           RECLOCK("SRA",.F.) 
            
            If  SRA->RA_CATFUNC = "P"             
                SRA->RA_SEGUROV := "01"
            Else    
				SRA->RA_SEGUROV := "02"
			Endif 	                   
		   MSUNLOCK()               
           DBSELECTAREA("ZZ5")   		
        Endif

        IF Dbseek(STRZERO(ZZ1->CODCOLIG,2) + ZZ1->CHAPA+ "0344") // SEGURO - HSBC
           DBSELECTAREA("SRA")       
           RECLOCK("SRA",.F.) 
            
            If  !SRA->RA_CATFUNC $ "E*G"             
                SRA->RA_SEGUROV := "03"
            Else    
				SRA->RA_SEGUROV := "04"
			Endif 	                   
		   MSUNLOCK()               
           DBSELECTAREA("ZZ5")   		
        Endif                 
        
        
 //DBSELECTAREA("SRA")   		
 //DBSKIP()  	

         
   DBSELECTAREA("ZZ1")
   DBSKIP()
   INCPROC()

 ENDDO

 DBSELECTAREA("ZZ1")
 DBCLOSEAREA()

 DBSELECTAREA("ZZ2")
 DBCLOSEAREA()
 
 DBSELECTAREA("ZZ3")
 DBCLOSEAREA()
 
 DBSELECTAREA("ZZ4")
 DBCLOSEAREA()

 DBSELECTAREA("ZZ5") 
 DBCLOSEAREA()
 

					MsgAlert( 'Ocorreram problemas na tentativa de deleo do arquivo '+AllTrim( cNomeArq )+'.' )
				EndIf
			EndIf
		Else
			MsgAlert( 'Ocorreram problemas no fechamento do arquivo '+AllTrim( cNomeArq )+'.' )
		EndIf
	EndIf
	
EndIf

Return NIL



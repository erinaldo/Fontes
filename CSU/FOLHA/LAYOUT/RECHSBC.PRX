Static Function GeraLinhas( aTipo, lQuebra )
Local cLinha     := ''
Local nTamMaxLin := 240 // 250
Local nI         := 0

For nI := 1 To Len( aTipo )
	bAux      := &( '{ || ' + aTipo[nI][7] + ' } ' )
	
	cTipo     := aTipo[nI][4]
	nTamanho  := aTipo[nI][5]
	nDecimal  := aTipo[nI][6]
	
	uConteudo := EVal( bAux )
	uConteudo := IIf( ValType( uConteudo ) == 'U' , '', EverChar( uConteudo ) )
	
	If     cTipo == 'C'
		uConteudo := PADR( SubStr( AllTrim( uConteudo ), 1, nTamanho ), nTamanho )
		
	ElseIf cTipo == 'N'
		uConteudo := StrZero( Val( uConteudo ) * (10^nDecimal) , nTamanho )
		
	EndIf
	
	cLinha += uConteudo
	
Next


If lQuebra
   cLinha += Replicate( ' ', nTamMaxLin - Len( cLinha ) ) + cQuebraLin
Else
   cLinha += Replicate( ' ', nTamMaxLin - Len( cLinha ) ) 
Endif
//If ( nPos := Ascan(aTipo,{ |x| &(x[7]) == "9" })) > 0
   //cLinha += Replicate( ' ', nTamMaxLin - Len( cLinha ) ) 
//Else
   //cLinha += Replicate( ' ', nTamMaxLin - Len( cLinha ) ) + cQuebraLin
//Endif

Return cLinha



Static Function CMPAtuRC3()

Local aRC3   := {}
Local aEstrut:= {}
Local i      := 0
Local j      := 0
Local lRC3	 := .F.
Local cTexto := ""
Local cAlias := ""

aEstrut := { "RC3_FILIAL" , "RC3_ORIGEM" , "RC3_CODIGO"      , "RC3_DESCR" , "RC3_SEQFOR" , "RC3_SEQPAI" , "RC3_TIPO" , "RC3_TPRESU" , "RC3_RESULT"   , "RC3_OPERA1" , "RC3_TPFM01" , "RC3_FORM01" , "RC3_OPERA2" , "RC3_TPFM02" , "RC3_FORM02" , "RC3_OPERA3" , "RC3_RESOU1" , "RC3_RESOU2" }
aAdd(aRC3, { "  "         , "U_"         , "BENEF.V.REFEICA" , ""          , "0010"       , ""           , ".EXE."    , "5"          , "U_fDescVr() " , ""           , "1"          , ""           , ""           , "1"          , ""           , ".END."      , ""           , ""           })
aAdd(aRC3, { "  "         , "U_"         , "BENEF.V.ALIMENT" , ""          , "0010"       , ""           , ".EXE."    , "5"          , "U_fDescVa() " , ""           , "1"          , ""           , ""           , "1"          , ""           , ".END."      , ""           , ""           })
aAdd(aRC3, { "  "         , "U_"         , "BENEF.V.TRANSPO" , ""          , "0010"       , ""           , ".EXE."    , "5"          , "U_fDescVt() " , ""           , "1"          , ""           , ""           , "1"          , ""           , ".END."      , ""           , ""           })

ProcRegua(Len(aRC3))

dbSelectArea("RC3")
dbSetOrder(1)
For i:= 1 To Len(aRC3)
	If !Empty(aRC3[i,2])
		If !dbSeek(aRC3[i,1]+aRC3[i,2]+aRC3[i,3])
			lRC3 := .T.
			If !(aRC3[i,2]+aRC3[i,3]$cAlias)
				cAlias += aRC3[i,2]+aRC3[i,3]+"/"
			EndIf
			RecLock("RC3",.T.)
			For j:=1 To Len(aRC3[i])
				If !Empty(FieldName(FieldPos(aEstrut[j])))
					FieldPut(FieldPos(aEstrut[j]),aRC3[i,j])
				EndIf
			Next j

			dbCommit()
			MsUnLock()
			IncProc(OemToAnsi("Atualizando RC3..."))
		EndIf
	EndIf
Next i

If lRC3
	cTexto := OemToAnsi("RC3 Atualizado..: ")+cAlias+CHR(13)+CHR(10)
EndIf

Return cTexto


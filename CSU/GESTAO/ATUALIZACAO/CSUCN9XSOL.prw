User Function CSUCN9NOME()
Local _aArea     := GetArea()
Local _cRetnome
Local _aRetUser

PswOrder(2)
If PswSeek(M->CN9_X_SOLI,.T.)                     // Efetuo a pesquisa, definindo se pesquiso usuário ou grupo
	_aRetUser := PswRet(1)                   	  // Obtenho o resultado conforme vetor
	_cRetnome := Upper(_aRetUser[1][4])       	  // Nome Completo do Usuario
Else
	_cRetnome := M->CN9_X_NSOL
EndIf

RestArea( _aArea )

Return( _cRetnome )             
                     


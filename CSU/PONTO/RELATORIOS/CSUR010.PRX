	    @ Li,0 PSAY "Periodo de Fechamento: " + DtoC(dPerIni) +' ate '+ DtoC(dPerFim)
	    Li++                                          
		@ Li,0 PSay "Codigo da Equipe.....: " + cOldEquipe + " - " + GetAdvFval("SZM","ZM_DESCR",xFilial("SZM")+cOldEquipe,1," ")
		Li++
		@ Li,000 PSay __PrtThinLine()
	    Li+=2
    EndIf   

    @ Li,000 PSAY Replicate('-',nTamLin)
    Li++
    @ Li,001 PSAY "T O T A I S"
    Li++
    Li++

	If Len(aTotChk) % 2 # 0
		aAdd(aTotChk, {Space(Len(aTotChk[1,1])),Space(Len(aTotChk[1,2])),0,0} )
	Endif
	If nImpHrs == 1 .or. nImpHrs == 3
		cDet := 'Cod Descricao                  Calc.      Infor.   Cod Descricao                Calc.      Infor.'
	ElseIf nImpHrs == 2
		cDet := 'Cod Descricao                             Infor.   Cod Descricao                           Infor.'
	Endif
    @ Li,000 PSAY cDet
    Li++

    @ Li,000 PSAY Replicate('=',48)+Space(2)+Replicate('=',48)
    Li++

	nMetade := Len(aTotChk) / 2
	For nX := 1 To Len(aTotChk)/2
		cDet := aTotChk[nX,1]+Space(1)
		cDet += aTotChk[nX,2]+Space(1)

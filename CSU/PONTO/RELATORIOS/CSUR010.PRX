Li := 60
For nX := 1 to Len(aDadosFunc)
	// Imprime Cabecalho da Capa de Lote
	If 	Li >= 60
		Li := 0
	    @ Li,0 PSAY "Capa de Lote do Espelho de Ponto"
	    Li++                                                                         '

	    //@ Li,0 PSAY "Supervisor...........: " + aDadosFunc[1,1] +"-"+ PadR(aDadosFunc[1,2],40)
	    @ Li,0 PSAY "Supervisor...........: " + GetAdvFval("SZM","ZM_NOMES",xFilial("SZM")+aDadosFunc[nX,3],1," ")

	    Li++                                          
	    @ Li,0 PSAY "Periodo de Fechamento: " + DtoC(dPerIni) +' ate '+ DtoC(dPerFim)
	    Li++                                          
		@ Li,0 PSay "Codigo da Equipe.....: " + cOldEquipe + " - " + GetAdvFval("SZM","ZM_DESCR",xFilial("SZM")+cOldEquipe,1," ")
		Li++
		@ Li,000 PSay __PrtThinLine()
	    Li+=2
	Endif
	@ Li,0 Psay Space(17) + "_________________________________________             _________________________________________"		
	  Li+=1
	@ Li,0 PSay " Assinaturas:" + Space(4) + aDadosFunc[nX,1] +"-"+ PadR(aDadosFunc[nX,2],40)+Space(19)+"GESTOR DO PONTO"  
    Li+=1
Next nX

//-- Se existirem totais, e se for selecionada sua impress„o, ser„o impressos.
If Len(aTotChk) > 0 .and. nImpHrs # 4
    aSort(aTotChk,,,{|x,y| x[1] < y[1]})
    
    If Li > 50
		Li := 0
	    @ Li,0 PSAY "Capa de Lote do Espelho de Ponto"
	    Li++                                          

	    //@ Li,0 PSAY "Supervisor...........: " + aDadosFunc[1,1] +"-"+ PadR(aDadosFunc[1,2],40)
	    @ Li,0 PSAY "Supervisor...........: " + GetAdvFval("SZM","ZM_NOMES",xFilial("SZM")+aDadosFunc[1,3],1," ")


	ImprEsp(' ','C',,nTamLin , nColunas ,  nTamanho , lTerminal, 2 )
	ImprEsp(' ','C',,nTamLin , nColunas ,  nTamanho , lTerminal, 2 )
	ImprEsp(' ','C',,nTamLin , nColunas ,  nTamanho , lTerminal, 2 )

	ImprEsp(Space(15) + Replicate('-',48)+Space(2)+Replicate('-',48),'C',,nTamLin , nColunas ,  nTamanho , lTerminal, 2 )
	cDet := Space(15) + "                  Coordenador                   " + Space(02)
	cDet +=             "                  Gerente                       "  
	ImprEsp(cDet,'C',,nTamLin , nColunas ,  nTamanho , lTerminal,2 ) 
	
	@ Li,0 PSay " Aprovação conforme politica corporativa - absenteísmo (Vo1 código 07.06.02.02) e "
    Li++
	@ Li,0 PSay " politicas corporativas de horas extras (Vo1 código 07.06.01) "
    Li+=2
	*/

    // Reinicializa Variavel
    aTotChk := {}
Endif

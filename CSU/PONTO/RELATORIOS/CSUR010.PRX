	nMetade := Len(aTotChk) / 2
	For nX := 1 To Len(aTotChk)/2
		cDet := aTotChk[nX,1]+Space(1)
		cDet += aTotChk[nX,2]+Space(1)
		cDet += Transform(aTotChk[nX,3], '@E 9999,999.99')+Space(1)
		cDet += Transform(aTotChk[nX,4], '@E 9999,999.99')+Space(2)


Static Function xAddToFile( cLog, cToFile )

Local nHdl  := -1
Local cBuff := cLog

If File( cToFile )

   nHdl := FOpen( cToFile, 1 )

   If nHdl >= 0

       FSeek( nHdl, 0, 2 )

   EndIf

Else

   nHdl := FCreate( cToFile )

EndIf

If nHdl >= 0

   FWrite( nHdl,  cBuff, Len( cBuff ) )

EndIf

FClose( nHdl )

Return


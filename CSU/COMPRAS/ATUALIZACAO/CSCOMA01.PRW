	If ( Ascan(aGrupo,"*") == 0 )
		cFiltro := "C7_FILIAL=='"+xFilial("SC7")+"'.And.(C7_GRUPCOM=='"+Space(Len(SC7->C7_GRUPCOM))+"' "
		aEval(aGrupo,{|x| cFiltro+= ".Or.C7_GRUPCOM=='"+x+"'"})
		cFiltro += ") .And. C7_CONAPRO =='L' .and. C7_ENCER <> 'E' "
	EndIf

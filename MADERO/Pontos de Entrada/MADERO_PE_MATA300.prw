User Function MA300OK()
Local lExec:=IIF(AllTrim(FunName())=="FAT300",.T.,.F.)

	// -> Verifica se executa o ponto de entrada 
	If lExec 
		mv_par01 := _aMA300OK[1]   	// Armazem inicial
		mv_par02 := _aMA300OK[2]   	// Armazem Final
		mv_par03 := _aMA300OK[3]   	// Produto inicial
		mv_par04 := _aMA300OK[4]   	// Produto final
		mv_par05 := _aMA300OK[5]    // Zera o saldo de produtos (MOD)
		mv_par06 := _aMA300OK[6]	// zera o cm (MOD)                    	
		mv_par07 := _aMA300OK[7]    // Nao
		mv_par08 := _aMA300OK[8] 	// Não seleciona filiais
	EndIf	
	
Return(.T.) 

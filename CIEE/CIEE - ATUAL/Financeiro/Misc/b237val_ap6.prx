   _Valor := StrZero(Val(SubStr(SE2->E2_CODBAR,06,14)),15)
ElseIf !Empty(SE2->E2_LD)
   _Valor := StrZero(Val(SubStr(SE2->E2_LD,34,14)),15)
Endif


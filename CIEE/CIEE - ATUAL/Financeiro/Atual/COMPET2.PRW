USER FUNCTION COMPET2()

Local _cRet	:= .T. 

IF VAL(SUBSTR(M->E2_XCOMPET,1,2)) < 01 .OR. VAL(SUBSTR(M->E2_XCOMPET,1,2)) > 12
	MSGINFO("Mês Incorreto. Digite novamente !") 
	_cRet	:= .F.
ELSE
	If VAL(SUBSTR(M->E2_XCOMPET,4,4)) < 2012 .OR. VAL(SUBSTR(M->E2_XCOMPET,4,4)) > 2100
		MSGINFO("Ano Incorreto. Digite novamente !")
		_cRet	:= .F.
	Endif
ENDIF 	
	
RETURN(_cRet)     

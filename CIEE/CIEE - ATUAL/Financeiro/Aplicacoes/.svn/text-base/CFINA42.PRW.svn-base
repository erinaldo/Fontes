User Function CFINA42A()

_aArea	:= GetArea()
_cRet	:= .T.

_cQuery	:= "SELECT COUNT(ZS_COD) REGSZS "
_cQuery	+= "FROM " + RetSqlName("SZS")+" "
_cQuery += "WHERE D_E_L_E_T_ <> '*' "
_cQuery += "AND ZS_COD = '"+M->ZT_COD+"' "
TCQUERY _cQuery ALIAS "TMPSZT" NEW

If M->ZT_BLOQ == "S"
	If TMPSZT->REGSZS > 0
		_cMsg := "Nใo ้ permitido Bloquear!!!"+CHR(10)+CHR(13)
		_cMsg += "C๓digo Vinculado a uma Reserva."
		MsgBox(OemToAnsi(_cMsg), "Aviso", "ALERT")
		_cRet := .F.
	EndIf
EndIf

TMPSZT->(DbCloseArea())
RestArea(_aArea)

Return(_cRet)
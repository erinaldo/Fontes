User Function AJUS_CTT()

DbSelectArea("CTT")
DbSetOrder(1)
DbGotop()

Do While !EOF()
	RecLock("CTT",.F.)
	CTT->CTT_DP06 := DP06()
	MsUnLock()

	DbSelectArea("CTT")
	DbSkip()
EndDo

Return


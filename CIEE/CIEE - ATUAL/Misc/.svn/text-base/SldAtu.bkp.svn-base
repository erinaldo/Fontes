SELECT
B1_CODANT "COD. ANT.",
SUBSTRING(B2_COD,  1, 08) "CÓDIGO",
SUBSTRING(B1_DESC, 1, 20) + '  ' DESCRIÇÃO,
B2_LOCAL "LOCAL",
STR(B2_QATU, 10, 2) "SLD ATUAL (1ª UM)",
STR(B2_QTSEGUM, 10, 2) "SLD ATUAL (2ª UM)",
STR(B2_VATU1, 10, 2) "SLD ATUAL ($)"
FROM SB1010 B1, SB2010 B2
WHERE 
B1_COD = B2_COD AND
B2_QATU != 0 AND
B1.D_E_L_E_T_ <> '*' AND
B2.D_E_L_E_T_ <> '*'
ORDER BY 1
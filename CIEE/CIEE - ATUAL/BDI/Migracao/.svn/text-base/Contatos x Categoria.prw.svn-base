Static Function RunCont

Local nTamFile, nTamLin, cBuffer, nBtLidos

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Abertura do arquivo texto                                           ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ

FT_FUSE("C:\Ap8\Protheus_Data\CSV\Contatos X Categorias.txt")
FT_FGOTOP()

nRec := 0
While !FT_FEOF()
	nRec++
	FT_FSKIP()
End

ProcRegua(nRec) 

FT_FGOTOP()

FT_FSKIP()

_cCodigo := ""
_cCateg  := ""

While !FT_FEOF()

    IncProc()
	cBuffer := Alltrim(FT_FREADLN())
	_cID   :=	Substr(cBuffer,1,(At(";",cBuffer)-1))

	cBuffer :=	Substr(cBuffer,(At(";",cBuffer)+1),500)
	_cDesc  := 	alltrim(cBuffer)
	
	dbSelectArea("SU5")
	dbSetOrder(9) //ID
	If dbSeek(xFilial("SU5")+_cID)
		_cCodigo := SU5->U5_CODCONT
	Else
		_cCodigo := ""
	EndIf

	dbSelectArea("SZW")
	dbSetOrder(2) //Descricao
	If dbSeek(xFilial("SZW")+_cDesc)
		_cCateg := SZW->ZW_COD
	Else
		_cCateg := ""
	EndIf

	dbSelectArea("SZY")
	RecLock("SZY",.T.)
	SZY->ZY_FILIAL  := "01"
	SZY->ZY_COD     := _cCateg      //Codigo da Categoria
	SZY->ZY_DESC    := _cDesc
	SZY->ZY_CODCONT := _cCodigo
	SZY->ZY_ID      := _cID
	SZY->ZY_ITEM    := ""
	MSUnLock()

	FT_FSKIP()
EndDo

FT_FUSE()

Close(oLeTxt)

Return
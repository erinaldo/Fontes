If cUser == Nil
	cUser := Substr(cUsuario,7,15)
EndIf

PswOrder(2)
If PswSeek(cUser)
	aGrupos  := PswRet(1)[1][10]
EndIf

dbSelectArea(cAlias)
dbSetOrder(cSavOrd)

Return aGrupos







If Type("lMsErroAuto") == "L"
	lErroOld := lMsErroAuto
Else
	Private lMsErroAuto := .F. 	
EndIf
//-- Prepara campos para a função: F050TotMes
M->E2_VENCREA 		:= SE2->E2_VENCREA

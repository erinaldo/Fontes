				ED_CALCISS With "N"	, 	;
				ED_CALCINS With "N"	,	;
				ED_CALCCSL With "N"  ,	;
				ED_CALCCOF With "N"  ,  ;
				ED_CALCPIS With "N"  ,	;
				ED_DESCRIC With "CIDE", ;

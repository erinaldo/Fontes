					FVerMinImp(m->e2_basepis,.T.)
				Else
					FVerMinImp(m->e2_valor,.T.)
				Endif
				M->E2_VLCRUZ:=Round(NoRound(xMoeda(M->E2_VALOR,M->E2_MOEDA,1,M->E2_EMISSAO,MsDecimais(1)+1,M->E2_TXMOEDA),MsDecimais(1)+1),MsDecimais(1))
		EndCase
	Endif
EndIf

Return( .T. )



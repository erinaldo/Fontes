				EndIf				

	   /*	   If (nVlr_Rtd_Tot 	<= 	nVlMinImp)
					F050DelPCC()  
				EndIf  */
			EndIf  			
		EndIf  
	Else 
		Help(" ",1,"NOATUARET"     ,, STR0169  + cPre_Rtr + cNum_Rtr      , 1,0)  //"Titulo Retentor - Não Atualizado: "			    
		lRet := .F.

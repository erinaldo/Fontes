Static Function VldSelCtr(aLbxCtr,aContratos)
LOCAL nSelOK := 0

aEval(aLbxCtr,{|x| If(x[1]:cName == "LBOK",++nSelOK,0)})

If nSelOK == 0
	Help(" ",1, "FA050VLDC",, STR0199, 4,0)

	Return .f.
ElseIf nSelOK > 1
	Help(" ",1, "FA050VLDC",, STR0200, 4,0)

	Return .f.
Endif

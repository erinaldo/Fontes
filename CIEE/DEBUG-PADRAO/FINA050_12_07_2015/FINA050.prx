cMoeda := Alltrim(Str(m->e2_moeda))
dbSelectArea("SX3")
nRec := Recno()
dbSetOrder(2)
If !dbSeek("M2_MOEDA"+cMoeda)
	Help ( " ", 1, "SEMMOEDA" )
	lRet := .F.
EndIf

dbSelectArea("SA6")
dbSetOrder(1)
dbSeek(xFilial("SA6") + cBancoAdt + cAgenciaAdt + cNumCon)
If SA6->A6_MOEDA <> 1 .and. M->E2_TIPO == MVPAGANT .and. SA6->A6_MOEDA != M->E2_MOEDA .and. !lMoedBco 
	Help ( " ", 1, "MOEDDIF" )
	lRet := .F.
EndIf


dbGoto(nRec)
dbSelectArea(cAlias)
dbSetOrder(nOrder)

Return lRet



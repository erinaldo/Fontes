If SE2->E2_EMISSAO >= dLastPcc .And. !lEmpPub
	nVlMinImp := 0
EndIf

dbSelectArea(cAlias)
RecLock(cAlias)
SE2->E2_NOMFOR	 := SA2->A2_NREDUZ
SE2->E2_EMIS1	 := IIf(Type("dDataEmis1") # "U",IIf(!Empty(dDataEmis1),dDataEmis1,dDataBase),dDataBase)
SE2->E2_VENCORI := E2_VENCTO
SE2->E2_SALDO	 := E2_VALOR

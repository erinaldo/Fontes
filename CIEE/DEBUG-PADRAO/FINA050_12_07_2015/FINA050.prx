				Replace A2_LOJA	With cLojaImp
				Replace A2_MUN 	With "."
				Replace A2_EST 	With SuperGetMv("MV_ESTADO")
				Replace A2_BAIRRO With "."
				Replace A2_END 	With "."
			EndIF
			dNextMes := Month(SE2->E2_EMISSAO)+1

			//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
			RecLock("SE2",.T.)
			SE2->E2_FILIAL		:= cFilial
			SE2->E2_PREFIXO 	:= cPrefixo
			SE2->E2_NUM			:= cNum
			SE2->E2_PARCELA 	:= cParcCIDE
			SE2->E2_NATUREZ 	:= AllTrim(cCIDE)
			SE2->E2_TIPO 		:= "CID"

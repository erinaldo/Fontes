If !lDesdobr .And. SE2->E2_MULTNAT # "1"
	If SE2->E2_TIPO $ MVPAGANT
		PcoDetLan("000002","02","FINA050")	// Tipo PA
	Else
		PcoDetLan("000002","01","FINA050")
	EndIf

*/
Function Fa050SX1Equ()
Local aArea := GetArea()
SX1->(dbSetOrder(1))
If SX1->(dbSeek("FIN050    "+"05")) .and. Empty(SX1->X1_VALID)
	RecLock("SX1",.F.)
    X1_PRESEL := 2
	X1_VALID  := "Fa050Perg5()"
	MsUnLock()
EndIf
RestArea(aArea)

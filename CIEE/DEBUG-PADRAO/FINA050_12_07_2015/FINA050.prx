Function FA050VldCp() // Validacao da inclusao NVS
Local lRet := .T.
Local aCpoNVS := {}
Local nI
Local lTel1 := IsInCallStack("FA050Tel1")  

If Trim(SED->ED_GRPNAT) == "1"
	aCpoNVS := { "NVS_GRPCLI",/*"NVS_PROCES",*/"NVS_CLIENT","NVS_LOJA", ;

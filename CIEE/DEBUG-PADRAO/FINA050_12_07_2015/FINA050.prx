	aCpoNVS := { /*"NVS_PROCES",*/"NVS_PARTIC","NVS_HISTOR"}
ElseIf Trim(SED->ED_GRPNAT) == "5"
	aCpoNVS := { "NVS_TABRAT" }
EndIf

If !lTel1
	aAdd(aCpoNVS,"NVS_NATUR")
EndIf 
   
For nI := 1 To Len(aCpoNVS)
	If Empty(M->&(aCpoNVS[nI]))

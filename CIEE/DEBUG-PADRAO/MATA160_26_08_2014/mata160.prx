Local aArea	     := GetArea()
Local nPosNumCot := aScan(aCotacao[1][1],{|x| Trim(x[1])=="C8_NUM"})
Local nPosTxFin  := aScan(aCotacao[1][1],{|x| Trim(x[1])=="C8_TAXAFIN"})

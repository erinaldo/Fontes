Function A160GValid()

Local uReadVar 	:= &(ReadVar())
Local nLinAudit := oGrade:nPosLinO
Local nLinha    := oGrade:oGetdados:oBrowse:Nat
Local nColuna   := oGrade:oGetdados:oBrowse:nColpos
Local nPPropSCE := aScan(oGrade:aHeadAux,{|x| Trim(x[2])=="CE_NUMPRO"}) 
Local nPFornSCE := aScan(oGrade:aHeadAux,{|x| Trim(x[2])=="CE_FORNECE"}) 
Local nPLojaSCE := aScan(oGrade:aHeadAux,{|x| Trim(x[2])=="CE_LOJA"})  
Local nPICotSCE := aScan(oGrade:aHeadAux,{|x| Trim(x[2])=="CE_ITEMCOT"}) 
Local nItemGrd  := 0
Local nSldProp  := 0
Local nX        := 0
Local nY        := 0
Local nScan     := 0
Local lRet 		:= .T.
Local lYesCot   := .F.

Local aHlpInfP := {"Este Item n? foi gerado nesta cota豫o, ","somente os itens que constam nesta cota?,"? podem ser informados.                "}
Local aHlpInfE := {"This Item was not generated in this quot","ation, only the items that they consist ","in this quote they can be informed.     "}  
Local aHlpInfS := {"Este item no se gener?en esta cotizacio","n, s?o los itens que consisten en esta ","cotizacion que ellos pueden informarse. "} 
Local aHlpSldP := {"A quantidade informada neste item somada","a quantidade distribuida entre as demais"," propostas, n? devem exceder a quantida","de original da cota豫o.                 "}
Local aHlpSldE := {"The amount informed in this added item  ","the amount distributed among the other  ","proposals,they should not exceed the    ","original amount of the quotation.       "} 
Local aHlpSldS := {"La cantidad informada en esto item      ","agreg?la cantidad distribuy?entre las ","otras propuestas, ellos no deben exceder","la cantidad original de la cotizacion.  "}

PutHelp("PA160QTDGRD" , aHlpInfP, aHlpInfE, aHlpInfS,.F.)
PutHelp("PA160SLDGRD" , aHlpSldP, aHlpSldE, aHlpSLdS,.F.)

nScan := aScan(aCotaGrade, {|z| z[1] + z[2] + z[3] + z[4] == ;
oGrade:aColsAux[nLinAudit][nPFornSCE] + oGrade:aColsAux[nLinAudit][nPLojaSCE] + oGrade:aColsAux[nLinAudit][nPPropSCE] + oGrade:aColsAux[nLinAudit][nPICotSCE] })

//旼컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴?
//?otina para calcular o Saldo da peca (Item da Grade) entre as demais propostas dos fornecedores      ?
//? calculo do saldo e necessario pois a soma por item da grade em todas as propostas dos fornecedores ?
//?ao pode exceder a quantidade original no SC8-Atencao nao sera considerado o item a validar na rotina?
//읕컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴?
For nX := 1 To Len(aCotaGrade)
	If Len(aCotaGrade[nX][6]) > 0 .And. nX <> nScan
		For nY:= 1 to Len(aCotaGrade[nX][6])
			If AllTrim(aCotaGrade[nX, 6, nY, 1]) == oGrade:GetNameProd(AllTrim(aCotaGrade[nScan, 5]),nLinha,nColuna)
				nSldProp += aCotaGrade[nX, 6, nY, 2]				
			EndIf
		Next nY
	EndIf
Next nX

//旼컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴?
//?alida a Quantidade digitada do Item do Produto de grade no Grid de grade, a quantidade digitada nao ?
//?ode exceder a quantidade original da cotacao na tabela SC8.                                         ?
//읕컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴?
For nItemGrd := 1 To Len(aCotaGrade[nScan][6])
	
	If AllTrim(aCotaGrade[nScan, 6, nItemGrd, 1]) == oGrade:GetNameProd(AllTrim(aCotaGrade[nScan, 5]),nLinha,nColuna)
		
		If oGrade:cCpo == "CE_QUANT"
			
			If uReadVar + nSldProp > aCotaGrade[nScan][6][nItemGrd][6]
				Help(" ",1,"A160SLDGRD")
				lRet:=.F.
			EndIf
			
		EndIf
		
		lYesCot := .T.
		
		Exit
		
	EndIf
	
Next nItemGrd

//旼컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴?
//?e o Item do Produto de grade apresentado no Grid nao existir na cotacao original SC8 a validacao    ?
//?mpede que seja digitado dados para o mesmo.                                                         ?
//읕컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴?
If !lYesCot
	Help(" ",1,"A160QTDGRD")
	lRet:=.F.
EndIf
	
Return(lRet)


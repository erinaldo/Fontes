aAdd( aSX3, {'SZB','01','ZB_FILIAL','C',2,0,'Filial','Sucursal','Branch','Filial do Sistema','Sucursal del Sistema','System Branch','@!','',Chr(128) + Chr(128) + Chr(128) + Chr(128) + Chr(128) +Chr(128) + Chr(128) + Chr(128) + Chr(128) + Chr(128) +Chr(128) + Chr(128) + Chr(128) + Chr(128) + Chr(128),'','',0,	Chr(128) + Chr(128),'','','','N','','','','','','','','','','','033','',''} )
